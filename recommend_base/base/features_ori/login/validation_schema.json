{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "LoginInput",
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "format": "email"
      },
      "password": {
        "type": "string",
        "minLength": 8,
        "pattern": "(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[!@#$%^&*])"
      }
    },
    "required": ["email", "password"]
  }
  